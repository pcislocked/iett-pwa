    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^25.3.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.8.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.40",
