{
  "name": "iett-pwa",
  "private": true,
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "node ./node_modules/typescript/bin/tsc --noEmit",
